<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Warna background -->
    <solid android:color="#E8F3E2"/>

    <!-- Outline (garis pinggir) -->
    <stroke
        android:width="2dp"
        android:color="#9E9E9E" />

    <!-- Corner radius -->
    <corners android:radius="16dp" />

    <!-- Padding dalam -->
    <padding
        android:left="12dp"
        android:top="12dp"
        android:right="12dp"
        android:bottom="12dp" />
</shape>
